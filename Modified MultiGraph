import networkx as nx

G = nx.MultiGraph()  # Initialize MultiGraph

G.add_node(1)
G.add_node(2)
G.add_edges_from([(1, 2), (1, 2)])  # Adding multiple edges between nodes 1 and 2

try:
    # Your code that manipulates the graph
except nx.NetworkXError as e:
    print(f"NetworkXError: {e}")

# Display edges
print(G.edges())
